
m1 = []
m2 = []

for i in range(2):
    for j in range(2):
        m1.append(int(input("M1: ")))

print("\n\n")
        
for i in range(2):
    for j in range(2):
        m2.append(int(input("M2: ")))
        
print("\n")
        
print(m1)
print(m2)
print("\n")

m = []

m.append(m1[0] * m2[0] + m1[1] * m2[2])
m.append(m1[0] * m2[1] + m1[1] * m2[3])
m.append(m1[2] * m2[0] + m1[3] * m2[2])
m.append(m1[2] * m2[1] + m1[3] * m2[3])

print(m)
    
