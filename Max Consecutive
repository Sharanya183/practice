n = list(map(int, input(" Enter Numbers\n").split()))

ctr = 1
maximum = 0

for i in range(len(n)-1):
    if n[i] == n[i+1]:
        ctr = ctr + 1
        if ctr > maximum:
            maximum = ctr
    else:
        ctr = 1
        continue
    
    
    
print(f"Max Consecutive: {maximum}")







